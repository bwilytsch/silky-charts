{
  "name": "silky-charts",
  "description": "A silky smooth D3/React library",
  "version": "0.1.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/silky-charts.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "alpha": "standard-version --prerelease alpha",
    "build": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf cjs esm umd",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "lint": "eslint **/*.js --quiet",
    "postpublish": "git push origin HEAD --follow-tags",
    "prebuild": "npm run clean",
    "prerelease": "npm run build && git add -A && git commit -m 'Update build'",
    "release": "standard-version",
    "test": "npm run lint && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "files": [
    "cjs/*",
    "esm/*",
    "umd/*",
    "src"
  ],
  "keywords": [
    "testing",
    "react",
    "ui",
    "dom",
    "jsdom",
    "unit",
    "integration",
    "functional",
    "end-to-end",
    "e2e"
  ],
  "author": "Juan D. Gomez <dg.dave@gmail.com> (http://davidgomez.io/)",
  "license": "MIT",
  "dependencies": {
    "d3-axis": "^1.0.12",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.3",
    "d3-time-format": "^2.1.3",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "husky": "^1.3.1",
    "jest-styled-components": "^6.3.1",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.8.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.3",
    "standard-version": "^4.4.0",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "styled-components": ">= 4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davegomez/silky-charts"
  },
  "bugs": {
    "url": "https://github.com/davegomez/silky-charts/issues"
  },
  "homepage": "https://github.com/davegomez/silky-charts#readme"
}
